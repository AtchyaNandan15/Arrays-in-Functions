#include <iostream>

using namespace std;

void fillArray(string name1[], int place);

int main()
{
  string myName[14];
  int i;
  int places;
  
    fillArray(myName,places);

  for(i=0;i<4;i++){
      myName[i];
  
       
        cout<<myName[i]<<"\n";
}
  
}

void fillArray(string name1[], int place){
    
    int w;
    
        for(w=0;w<4;w++){
      cout<<"Enter a name";
      cin>>name1[w];
  }
        cout<<name1[w]<<"\t\n";

    
    cout<<"Enter the number of the name";
    cin>>place;
    
      
    cout<<"Enter the name you want to replace number";
    cin>>name1[place - 1];
    cout<<"\n";
    
    
    
}













